{
  "name": "cutcal",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint && npm run lint:styles",
    "lint:styles": "stylelint \"apps/**/*.scss\" && stylelint \"libs/**/*.scss\"",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:all": "nx affected:lint && nx affected:test && nx affected:e2e",
    "affected:dep-graph": "nx affected:dep-graph",
    "open:coverage": "for file in $(find . -wholename '*./coverage/**/index.html' -not -wholename **app/*); do open $file; done;",
    "get:coverage": "ng test --codeCoverage",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "stats": "webpack-bundle-analyzer dist/stats.json",
    "build:stats": "ng build --prod --stats-json && webpack-bundle-analyzer dist/stats.json",
    "kill:4200": "sudo kill $(sudo lsof -t -i:4200)"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.4",
    "@angular/cdk": "^9.1.0",
    "@angular/common": "^9.0.4",
    "@angular/compiler": "^9.0.4",
    "@angular/core": "^9.0.4",
    "@angular/fire": "^5.4.2",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "^9.0.4",
    "@angular/material": "^9.1.0",
    "@angular/platform-browser": "^9.0.4",
    "@angular/platform-browser-dynamic": "^9.0.4",
    "@angular/router": "^9.0.4",
    "@nestjs/common": "^6.11.8",
    "@nestjs/core": "^6.11.8",
    "@nestjs/platform-express": "^6.11.8",
    "@ngrx/effects": "^8.6.0",
    "@ngrx/router-store": "^8.6.0",
    "@ngrx/schematics": "^8.6.0",
    "@ngrx/store": "^8.6.0",
    "@ngrx/store-devtools": "^8.6.0",
    "@nrwl/angular": "9.0.4",
    "convert-units": "^2.3.4",
    "core-js": "^2.5.4",
    "firebase": "^7.9.3",
    "hammerjs": "^2.0.8",
    "highcharts": "^8.0.0",
    "highcharts-angular": "^2.4.0",
    "lodash": "^4.17.15",
    "ngrx": "0.0.1-security",
    "ngx-quicklink": "^0.2.0",
    "pluralize": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.5.4",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.900.4",
    "@angular-devkit/build-angular": "^0.900.4",
    "@angular-devkit/build-ng-packagr": "^0.900.4",
    "@angular/cli": "9.0.2",
    "@angular/compiler-cli": "^9.0.4",
    "@angular/language-service": "^9.0.4",
    "@nestjs/schematics": "^6.9.4",
    "@nestjs/testing": "^6.11.8",
    "@nrwl/cypress": "9.0.4",
    "@nrwl/jest": "9.0.4",
    "@nrwl/nest": "9.0.4",
    "@nrwl/node": "9.0.4",
    "@nrwl/schematics": "^8.12.9",
    "@nrwl/workspace": "9.0.4",
    "@types/jest": "24.0.9",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.29",
    "@types/pluralize": "0.0.29",
    "codelyzer": "^5.1.2",
    "cypress": "^3.8.2",
    "dotenv": "6.2.0",
    "eslint": "6.1.0",
    "firebase-tools": "^7.14.0",
    "fuzzy": "^0.1.3",
    "husky": "^4.2.3",
    "inquirer": "^6.5.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jest": "24.1.0",
    "jest-preset-angular": "^8.0.0",
    "ng-packagr": "^5.4.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "stylelint": "^13.2.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.14.2",
    "ts-jest": "24.0.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-defocus": "^2.0.6",
    "typescript": "~3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && git add . && npm run affected:all",
      "pre-push": "npm run lint && npm run test && npm run e2e"
    }
  }
}
