        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/cutcal/jest.config.js",
            "tsConfig": "apps/cutcal/tsconfig.spec.json",
            "setupFile": "apps/cutcal/src/test-setup.ts",
