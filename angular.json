{
  "version": 1,
  "projects": {
    "cutcal": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/cutcal",
      "sourceRoot": "apps/cutcal/src",
      "prefix": "cutcal",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/cutcal",
            "index": "apps/cutcal/src/index.html",
            "main": "apps/cutcal/src/main.ts",
            "polyfills": "apps/cutcal/src/polyfills.ts",
            "tsConfig": "apps/cutcal/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/cutcal/src/favicon.ico",
              "apps/cutcal/src/assets"
            ],
            "styles": [
              "apps/cutcal/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/cutcal/src/environments/environment.ts",
                  "with": "apps/cutcal/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "cutcal:build",
            "proxyConfig": "apps/cutcal/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "cutcal:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "cutcal:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/cutcal/tsconfig.app.json",
              "apps/cutcal/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/cutcal/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/cutcal/jest.config.js",
            "tsConfig": "apps/cutcal/tsconfig.spec.json",
            "setupFile": "apps/cutcal/src/test-setup.ts"
          }
        },
        "deploy": {
          "builder": "@angular/fire:deploy",
          "options": {}
        }
      }
    },
    "cutcal-e2e": {
      "root": "apps/cutcal-e2e",
      "sourceRoot": "apps/cutcal-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/cutcal-e2e/cypress.json",
