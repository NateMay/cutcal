              "algoliasearch",
              "algoliasearch-helper",
              "nouislider",
              "hogan.js",
              "highcharts"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/cutcal/src/environments/environment.ts",
                  "with": "apps/cutcal/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
